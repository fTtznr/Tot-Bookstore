{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { cartStore } from '@/store/cart';\nexport default {\n  name: \"Cart\",\n  data() {\n    return {\n      cart: null\n    };\n  },\n  created() {\n    this.cart = cartStore();\n    if (this.cart) this.cart = JSON.parse(this.cart);else this.cart = {};\n  },\n  methods: {\n    searchCart(book) {\n      let index = -1;\n      for (const [i, item] of this.cart.items.entries()) {\n        if (book.bookId === item.bookId) {\n          index = i;\n          break;\n        }\n      }\n      return index;\n    },\n    removeItem(book) {\n      let index = this.searchCart(book);\n      if (index != -1) this.cart.items.splice(index, 1);\n      localStorage.setItem(\"cart\", JSON.stringify(this.cart));\n    },\n    bookDetail(book) {\n      localStorage.setItem(\"book\", JSON.stringify(book));\n      this.$router.push(\"/book\");\n    },\n    increase(book) {\n      this.cart.items[this.searchCart(book)].qtde++;\n      localStorage.setItem(\"cart\", JSON.stringify(this.cart));\n    },\n    decrease(book) {\n      if (this.cart.items[this.searchCart(book)].qtde > 1) {\n        this.cart.items[this.searchCart(book)].qtde--;\n        localStorage.setItem(\"cart\", JSON.stringify(this.cart));\n      }\n    }\n  }\n};","map":{"version":3,"mappings":";AAgFA,SAASA,SAAQ,QAAS,cAAa;AAEvC,eAAc;EAEVC,IAAI,EAAE,MAAM;EAEZC,IAAI,GAAE;IAEF,OAAM;MAEFC,IAAI,EAAE;IAEV;EAEJ,CAAC;EAEDC,OAAO,GAAE;IAEL,IAAI,CAACD,IAAG,GAAIH,SAAS,EAAC;IAEtB,IAAG,IAAI,CAACG,IAAI,EAAE,IAAI,CAACA,IAAG,GAAIE,IAAI,CAACC,KAAK,CAAC,IAAI,CAACH,IAAI,OACzC,IAAI,CAACA,IAAG,GAAI,CAAC;EAEtB,CAAC;EAEDI,OAAO,EAAC;IAEJC,UAAU,CAACC,IAAI,EAAC;MAEZ,IAAIC,KAAI,GAAI,CAAC;MAEb,KAAI,MAAM,CAACC,CAAC,EAAEC,IAAI,KAAK,IAAI,CAACT,IAAI,CAACU,KAAK,CAACC,OAAO,EAAE,EAChD;QAEI,IAAGL,IAAI,CAACM,MAAK,KAAMH,IAAI,CAACG,MAAM,EAC9B;UAEIL,KAAI,GAAIC;UACR;QAEJ;MAEJ;MAEA,OAAOD,KAAI;IAEf,CAAC;IAEDM,UAAU,CAACP,IAAI,EAAC;MAEZ,IAAIC,KAAI,GAAI,IAAI,CAACF,UAAU,CAACC,IAAI;MAEhC,IAAGC,KAAI,IAAK,