\n\r\n                    <div class=\"cart-product-details icon\">\r\n                        Remover\r\n                    </div>\r\n\r\n                </div>\r\n                \r\n            </div>\r\n\r\n            <div class=\"cart-product\" v-for=\"book in cart.items\" :key=\"book.bookId\">\r\n                \r\n                <div class=\"cart-product-img\">\r\n                    <img :src=\"book.coverImg\"  @click=\"bookDetail(book)\">\r\n                </div>\r\n\r\n                <div class=\"cart-product-info\">\r\n\r\n                    <div class=\"cart-product-details\">\r\n                        {{book.title}}\r\n                    </div>\r\n\r\n                    <div class=\"cart-product-details\">\r\n                        {{book.price}}\r\n                    </div>\r\n\r\n                    <div class=\"cart-product-details\">\r\n                       <button @click=\"decrease(book)\">-</button> {{book.qtde}} <button @click=\"increase(book)\">+</button> \r\n                    </div>\r\n\r\n                    <div class=\"cart-product-details icon trash\">\r\n                        <i class=\"fa-solid fa-trash-can\" @click=\"removeItem(book)\" />\r\n                    </div>\r\n\r\n                </div>\r\n\r\n            </div> \r\n\r\n            <div class=\"center\" v-if=\"totalPrice !=0\"> Total: R$ {{ totalPrice }}</div>\r\n\r\n        </div>\r\n\r\n        <div v-else>\r\n\r\n            <h1 class=\"capital center\">Seu carrinho está vazio!</h1>\r\n\r\n        </div>\r\n                    \r\n    </div> \r\n\r\n</template>\r\n\r\n\r\n<script>\r\nimport { cartStore } from '@/store/cart'\r\n\r\nexport default{\r\n\r\n    name: \"Cart\",\r\n\r\n    data(){\r\n\r\n        return{\r\n\r\n            cart: null\r\n        \r\n        }\r\n\r\n    },\r\n\r\n    created(){\r\n\r\n        this.cart = cartStore()\r\n\r\n        if(this.cart) this.cart = JSON.parse(this.cart)\r\n        else this.cart = {}\r\n        \r\n    },\r\n\r\n    methods:{\r\n\r\n        searchCart(book){\r\n\r\n            let index = -1\r\n            \r\n            for(const [i, item] of this.cart.items.entries())\r\n            {\r\n\r\n                if(book.bookId === item.bookId) \r\n                {\r\n\r\n                    index = i\r\n                    break\r\n                \r\n                }\r\n\r\n            }\r\n\r\n            return index\r\n\r\n        },\r\n\r\n        removeItem(book){\r\n\r\n            let index = this.searchCart(book)\r\n\r\n            if(index != -1) this.cart.items.splice(index, 1)\r\n\r\n            localStorage.setItem(\"cart\", JSON.stringify(this.cart))\r\n\r\n        },\r\n\r\n        bookDetail(book){\r\n\r\n            localStorage.setItem(\"book\", JSON.stringify(book))\r\n            this.$router.push(\"/book\");\r\n\r\n        },\r\n\r\n        increase(book){\r\n\r\n            this.cart.items[this.searchCart(book)].qtde++\r\n\r\n            localStorage.setItem(\"cart\", JSON.stringify(this.cart))\r\n        },\r\n\r\n        decrease(book){\r\n\r\n            \r\n            if(this.cart.items[this.searchCart(book)].qtde > 1){\r\n\r\n                this.cart.items[this.searchCart(book)].qtde--\r\n                localStorage.setItem(\"cart\", JSON.stringify(this.cart))\r\n            \r\n            }\r\n                \r\n        }\r\n\r\n    }\r\n\r\n}\r\n\r\n</script>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}