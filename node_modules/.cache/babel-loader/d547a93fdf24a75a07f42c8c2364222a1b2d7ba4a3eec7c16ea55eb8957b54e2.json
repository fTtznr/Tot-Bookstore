nts: {\r\n        AddtoCart,\r\n        Confirmation \r\n    },\r\n\r\n    data(){\r\n\r\n        return{\r\n\r\n            book: null,\r\n            books: null,\r\n            session: null,\r\n            data: null\r\n\r\n        }\r\n\r\n    },\r\n\r\n    created(){\r\n\r\n        this.books = bookStore()\r\n        this.book = this.books.lastSeen\r\n        this.session = sessionStore()\r\n\r\n        this.data = {\r\n\r\n            title: \"Excluir\",\r\n            msg: \"Tem certeza que deseja excluir este Livro?\",\r\n            confirmOption: \"Excluir\"\r\n        }\r\n\r\n    },\r\n\r\n    methods:{\r\n\r\n        deleteBook(bookId){\r\n\r\n            this.books.deleteBook(bookId)\r\n\r\n            //confirmacao\r\n            //verificar o carrinhho\r\n\r\n            this.$router.push(\"/\");\r\n\r\n        }\r\n\r\n    }\r\n \r\n }\r\n\r\n</script>\r\n\r\n<style scoped>\r\n.label{\r\n    font-weight: bold;\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}